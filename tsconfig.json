{
  "compilerOptions": {
    "outDir": "/dist",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@fetchApi/*": [
        "./typescript/fetchApi/*"
      ],
      "@apps/*": [
        "./typescript/apps/*"
      ],
      "@FKSDB/*": [
        "./app/*"
      ],
      "@shared/*": [
        "./typescript/shared/*"
      ],
      "@translator/*": [
        "./app/Model/Localization/*"
      ],
      "@appsCollector/*": [
        "./typescript/appsCollector/*"
      ],
      "@inputConnector/*": [
        "./typescript/inputConnector/*"
      ],
      "@config": [
        "./typescript/config"
      ]
    }
  },
  "include": [
    "./**/*"
  ]
}
