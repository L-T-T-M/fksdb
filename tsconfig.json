{
  "compilerOptions": {
    "outDir": "/dist",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@fetchApi/*": [
        "./typescript/fetchApi/*"
      ],
      "@apps/*": [
        "./typescript/apps/*"
      ],
      "@shared/*": [
        "./typescript/shared/*"
      ],
      "@i18n/*": [
        "./typescript/i18n/*"
      ],
      "@appsCollector/*": [
        "./typescript/appsCollector/*"
      ],
      "@inputConnector/*": [
        "./typescript/inputConnector/*"
      ],
      "@config": [
        "./typescript/config"
      ]
    }
  },
  "include": [
    "./src/**/*"
  ]
}
