<!DOCTYPE html>
<html style="padding-top:0">
<head>
    <meta charset="UTF-8">
    <meta name="robots" content="{$robots}" n:ifset="$robots">

    <title>{block titleHeader}{$title} | FKSDB{/block}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {control cssLoader
        'css/bootstrap.v4.min.css' => [all],
        'css/style.css' => [screen],
        'css/fyziklani.css' => [all],
        }

    {block head}{/block}
</head>

<body style="padding-top:0">
<div id="wrap">
    <div class="container">
        {block #content}
        {/block}
    </div>
</div>

<div id="spinner"></div>

{control jsLoader
        'js/jquery.min.js',
        'js/jquery-ui.min.js',
        'js/bootstrap.v4.min.js',
        'js/nette.ajax.js',
        'js/tablesorter.min.js',
        'js/lib/react.min.js',
        'js/lib/react-dom.min.js',
        'js/dist/bundle.min.js'
            }

{control jsLoader:inline}

{control cssLoader}
</body>
</html>
