<!DOCTYPE html>
<html style="padding-top:0">
<head>
    <meta charset="UTF-8">
    <meta name="robots" content="{$robots}" n:ifset="$robots">

    <title>{block titleHeader}{$title} | FKSDB{/block}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {control cssLoader
        'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css' => [all],
        'css/style.css' => [screen],
        'css/fyziklani.css' => [all],
        }

    {block head}{/block}
</head>

<body style="padding-top:0">
<div id="wrap">
    <div class="container">
        {block #content}
        {/block}
    </div>
</div>

<div id="spinner"></div>

{control jsLoader
        'https://code.jquery.com/jquery-3.2.1.slim.min.js',
        'js/jquery.min.js',
        'js/jquery-ui.min.js',
        'https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js',
        'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js',
        'js/nette.ajax.js',
        'js/tablesorter.min.js',
        'js/lib/react.min.js',
        'js/lib/react-dom.min.js'
            }

{control jsLoader:inline}

{control cssLoader}
</body>
</html>
