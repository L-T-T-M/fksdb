<ul n:if="$allowedSeries" class="nav navbar-nav navbar-right">
    <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">{_'Série'} {$currentSeries}</a>
        <ul class="dropdown-menu">
            <li n:foreach="$allowedSeries as $series"{if $series==$currentSeries} class="active"{/if}><a href="{plink this series => $series}">{_'Série'} {$series}</a></li>
        </ul>
    </li>
</ul>    


