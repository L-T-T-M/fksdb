{snippet jsFiles}
    {varType array $files}
    {varType FKSDB\Components\Controls\Loaders\JavaScript\JavaScriptLoaderComponent $control}
    {varType string $basePath}
    {foreach $files as $file}
        {var $url = $file[$control::FILENAME]}
        {if $control::isRelative($url)}
            <script src="{$basePath}/{$url}"></script>
        {else}
            <script src="{$url}"></script>
        {/if}
    {/foreach}
{/snippet}
