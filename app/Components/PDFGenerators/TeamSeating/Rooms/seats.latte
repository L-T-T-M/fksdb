{define seats FKSDB\Models\ORM\Models\Fyziklani\Seating\RoomModel $room}
    {foreach $room->getSeats() as $row}
        {var FKSDB\Models\ORM\Models\Fyziklani\Seating\SeatModel $seat = FKSDB\Models\ORM\Models\Fyziklani\Seating\SeatModel::createFromActiveRow($row,$room->mapper)}
        {var $cor = 'translate(' . $seat->layout_x . ',' . $seat->layout_y . ')'}
        <g transform="{$cor}">
            {include seat 'seat'=>$seat}
        </g>
    {/foreach}
{/define}
